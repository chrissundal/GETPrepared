<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net9.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UseWPF>true</UseWPF>
        <ApplicationIcon>Icons\walk.ico</ApplicationIcon>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    </ItemGroup>

    <ItemGroup>
      <None Include="bin\Debug\net9.0-windows\Icons\walk.gif" />
      <None Include="bin\Debug\net9.0-windows\Icons\walk.png" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Icons\walk.png" />
      <Resource Include="Icons\walk.png" />
    </ItemGroup>

    <ItemGroup>
      <Resource Include="bin\Debug\net9.0-windows\Images\gear.png" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Images\" />
    </ItemGroup>

</Project>
